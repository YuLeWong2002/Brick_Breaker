<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="700.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="brickGame.MainMenuController">
   <Pane prefHeight="700.0" prefWidth="500.0" AnchorPane.bottomAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
      
         <ImageView fitHeight="700.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@Menu.png" />
            </image>
         </ImageView>
      </children>
   </Pane>

   <Button id="load" fx:id="load" layoutX="153.0" layoutY="323.0" onAction="#onClickLoad" prefHeight="54.0" prefWidth="194.0" text="Load" AnchorPane.leftAnchor="153.0" AnchorPane.topAnchor="323.0" />
   <Button id="newGame" fx:id="newGame" layoutX="152.0" layoutY="427.0" onAction="#onClickNewGame" prefHeight="54.0" prefWidth="194.0" text="Start Game" />

</AnchorPane>
